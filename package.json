{
  "name": "discordjs-template-ts",
  "version": "1.1.0",
  "description": "A template for discord.js v14 bots written in TypeScript",
  "main": "./dist/index.js",
  "keywords": [
    "discord.js",
    "discord",
    "typescript",
    "template",
    "v14"
  ],
  "license": "MIT",
  "author": {
    "name": "AtsumiFlex",
    "url": "https://github.com/AtsumiFlex"
  },
  "type": "commonjs",
  "readme": "README.md",
  "scripts": {
    "start": "nodemon ./dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "deploy": "npm run build && node ./dist/index.js"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "tslib": "^2.6.2",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "discord-api-types": "^0.37.76",
    "eslint": "^8.56.0",
    "eslint-config-neon": "^0.1.60",
    "eslint-formatter-pretty": "^6.0.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
